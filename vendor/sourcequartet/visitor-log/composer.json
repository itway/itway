{
    "name": "sourcequartet/visitor-log",
    "description": "A package for Laravel 5 to log all visitors",
	"keywords": [
		"laravel",
		"visitor",
		"log",
		"sentry"
	],
    "authors": [
        {
            "name": "Tony Briet",
            "email": "tbriet@gmail.com"
        },
        {
            "name": "Jones",
            "email": "admin@jonesboard.de"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/auth": "5.1.*",
        "illuminate/config": "5.1.*",
        "illuminate/support": "5.1.*",
        "illuminate/session": "5.1.*",
        "illuminate/database": "5.1.*",
        "illuminate/http": "5.1.*",
        "jenssegers/agent": "^2.1"
    },
    "require-dev":{
      "phpunit/phpunit": "~4.0",
      "phpspec/phpspec": "~2.1",
      "laracasts/testdummy": "~2.0",
      "orchestra/testbench": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "SourceQuartet\\VisitorLog\\": "src/SourceQuartet/VisitorLog"
        }
    },
    "autoload-dev": {
      "classmap": [
        "tests/TestCaseDB.php"
      ],
      "psr-4": {
        "SourceQuartet\\Tests\\": "tests/models"
      }
    },
    "license": "MIT"
}
